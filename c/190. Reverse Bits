int reverseBits(int n) {
    int result = 0;

    
    for (int i = 0; i < 32; i++) {
        //printf("n: %i, res: %i, factor: %u, i: %i\n", n, result, factor, i);
        result =  (result << 1) | ((n >> i) & 1);
        //(n >> i) binary digit at the ith position
        // & 1 because only want to return if the binary is 1 not 0
        //result << 1 because want to multiply by factor of 2 for each binary position in result
        //printf("n: %i, res: %i, i: %i\n", (n >> i), result, i);
    }
    return result;
}
