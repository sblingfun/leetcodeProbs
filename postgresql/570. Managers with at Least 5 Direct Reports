SELECT e1.name
FROM Employee e1
JOIN (SELECT managerId
        FROM Employee
        GROUP BY managerId
        HAVING COUNT(*) >= 5
    ) e2
ON e1.id = e2.managerId;
--INNER JOIN Employee e2 ON COUNT(e2.managerId = e1.id) > 5; --Aggregate functions are not allowed in join
--SELECT, FROM, GROUP BY, HAVING -- FOR Aggregate functions
--SELECT, FROM, JOIN (table) ON (condition)
