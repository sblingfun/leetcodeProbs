//Brute force metho
//Can implement solution which scans thorugh binary digits until first 0 is encountered

class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int[] returnArr = new int[nums.size()];

        for (int i = 0; i < nums.size(); i++) {
            if (nums.get(i) == 2) returnArr[i] = -1;
            else {
                int answer = 1;
                while (answer <= nums.get(i)) {
                    /*
                    if (answer + 1 == nums.get(i)) break;
                    else if (answer + answer == nums.get(i)) break;
                    else if (answer + answer + 1 == nums.get(i)) break;
                    */
                    int result = answer | (answer + 1);
                    if (result == nums.get(i)) break;
                    else {
                        answer++;
                    }
                }
                returnArr[i] = answer;
            }
        }
        return returnArr;
    }
}
