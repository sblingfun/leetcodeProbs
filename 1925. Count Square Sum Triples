
//Initial attempt - triple nested loop
//Solution slow

class Solution {
    public int countTriples(int n) {
        int retVal = 0;
        for (int i = 1; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                for (int k = i + 2; k <= n; k++) {
                    if (i * i + j * j == k * k) {
                        retVal += 2;
                    }
                }
            }
        }
        return retVal;
    }
}
